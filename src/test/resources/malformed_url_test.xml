<?xml version="1.0" encoding="UTF-8"?>

<slideshow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="urn:slideshow.Namespace"
           xsi:schemaLocation="urn:slideshow.Namespace PWS_Schema.xsd"
           author="alex tee" version="0.1">
    <defaults backgroundcolour="#faf347" font="Calibri" fontsize="16" fontcolour="#855797" linecolour="#fa44dd"
              fillcolour="#88cd02"/>
    <slide id="goodAudio">
        <audio urlname="bird1.wav" loop="false"/>
    </slide>
    <slide id="badAudio">
        <audio urlname="this isnt a url lol" starttime="0" loop="false"/>
    </slide>
    <slide id="goodVideo">
        <video urlname="bird.mov" starttime="0" loop="true" xstart="100" ystart="100"/>
    </slide>
    <slide id="badVideo">
        <video urlname="also not a url" loop="true" xstart="100" ystart="100"/>
    </slide>
    <slide id="goodImage">
        <image urlname="bird.png" xstart="100" ystart="100" width="10" height="10"/>
    </slide>
    <slide id="badImage">
        <image urlname="weeoowweoeowoow" xstart="100" ystart="100" width="10" height="10"/>
    </slide>
</slideshow>